{
  "name": "meNU",
  "description": "Quick links to MyNortheastern services.",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [ "tabs" ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/paw16.png",
    "32": "/images/paw32.png",
    "48": "/images/paw48.png",
    "128": "/images/paw128.png"
  }
}